# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_SESSION_TOKEN=your_session_token_here  # Optional, for temporary credentials
AWS_DEFAULT_REGION=us-west-2
AWS_PROFILE=default  # Optional, if using named profiles

# Bedrock Configuration
BEDROCK_REGION=us-west-2
BEDROCK_ORCHESTRATOR_MODEL=us.anthropic.claude-sonnet-4-20250514-v1:0
BEDROCK_TIMEOUT=600
BEDROCK_MAX_RETRIES=3

# Agent Configuration
AGENT_MAX_ITERATIONS=30
AGENT_TEMPERATURE=0.7
AGENT_MAX_TOKENS=4096
ENABLE_AGENT_THINKING=true
ENABLE_STREAMING=true

# Testing Configuration
TEST_ASSETS_DIR=test_assets
TEST_REPORTS_DIR=test_reports
ENABLE_PARALLEL_TESTING=false
MAX_CONCURRENT_TESTS=5
TEST_TIMEOUT_SECONDS=300

# Tool Configuration
BYPASS_TOOL_CONSENT=true  # Set to true for automated workflows

# Logging Configuration
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_FILE=logs/model_testing.log
ENABLE_CONSOLE_LOGGING=true
ENABLE_STRUCTURED_LOGGING=true

# Performance Configuration
ENABLE_CACHING=true
CACHE_DIRECTORY=.cache
ASSET_DOWNLOAD_TIMEOUT=60

# Security Configuration
VALIDATE_INPUTS=true
SANITIZE_OUTPUTS=true
USE_IAM_ROLES=true  # Recommended for EC2/ECS/Lambda

# Reporting Configuration
INCLUDE_RAW_RESPONSES=true
INCLUDE_PERFORMANCE_METRICS=true
INCLUDE_ERROR_TRACES=true
GENERATE_VISUALIZATIONS=false

# Feature Flags
ENABLE_ADVANCED_ANALYTICS=true
ENABLE_COST_TRACKING=true
ENABLE_PERFORMANCE_PROFILING=true
ENABLE_AUTO_RETRY=true
ENABLE_INCREMENTAL_REPORTING=true

# Optional: External Integrations
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
EMAIL_NOTIFICATION_ADDRESS=your-email@example.com
CLOUDWATCH_LOG_GROUP=/aws/model-testing
CLOUDWATCH_LOG_STREAM=model-testing-agent

# Optional: Model-Specific Overrides
AMAZON_NOVA_MODEL_ID=us.amazon.nova-omni-v1:0
CLAUDE_SONNET_MODEL_ID=us.anthropic.claude-sonnet-4-20250514-v1:0

# Optional: Advanced Settings
HTTP_PROXY=http://proxy.example.com:8080  # If behind proxy
HTTPS_PROXY=https://proxy.example.com:8080
NO_PROXY=localhost,127.0.0.1

# Optional: Development Settings
DEBUG_MODE=false
VERBOSE_LOGGING=false
ENABLE_PROFILING=false
PROFILE_OUTPUT_DIR=profiles